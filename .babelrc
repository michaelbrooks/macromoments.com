{
  "presets": ["es2015", "stage-0", "react"],
  "env": {
    "development": {
      "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
        [
          "react-transform", {
          "transforms": [
            {
              "transform" : "react-transform-hmr",
              "imports"   : ["react"],
              "locals"    : ["module"]
            }
          ]}
        ]
      ],
      "presets": ["es2015", "react", "stage-1"],
    },
    "production": {
      "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
      ],
      "presets": ["es2015", "react", "stage-1"],
    }
  }
}
